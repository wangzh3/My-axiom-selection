     => m1_subset_1(k4_subset_1(A,B,C),k1_zfmisc_1(A)) ) )).

fof(redefinition_k4_subset_1,axiom,(
    ! [A,B,C] :
      ( ( m1_subset_1(B,k1_zfmisc_1(A))
